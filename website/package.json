{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "routify -b && rollup -c",
    "dev": "run-p dev:*",
    "dev:rollup": "rollup -c -w",
    "dev:routify": "routify",
    "start": "sirv public --single --port 7000",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "jest",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "format": "prettier --write ./src/**/*.{svelte,ts}",
    "lint": "eslint './src/**/*.{svelte,ts}'",
    "lint:fix": "eslint --fix './src/**/*.{svelte,ts}'",
    "prelint": "npm run format"
  },
